-static unsigned int readLsb32(CCompressedFile &f)
-{
-    unsigned char num[4];
-
-    if(4==f.read(num, 4))
-        return (num[0])+(num[1]<<8)+(num[2]<<16)+(num[3]<<24);
-    else
-        return 0;
-}
-
-static unsigned int read32(CCompressedFile &f, bool msb)
-{
-    if(msb)
-    {
-        unsigned char num[4];
-
-        if(4==f.read(num, 4))
-            return (num[0]<<24)+(num[1]<<16)+(num[2]<<8)+(num[3]);
-        else
-            return 0;
-    }
-    else
-        return readLsb32(f);
-}
