+int KSMClient::restartStyleHint() const
+{
+    SmProp* p = property( SmRestartStyleHint );
+    if ( !p || qstrcmp( p->type, SmCARD8) || p->num_vals < 1)
+        return SmRestartIfRunning;
+    return *((int*)p->vals[0].value);
+}
