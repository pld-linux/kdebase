+{
+  kdDebug() << "slotChanged()\n";
+  setDirty(true);
+}
+
+PropsDlgSharePlugin::~PropsDlgSharePlugin()
+{
+  delete d;
+}
+
+void PropsDlgSharePlugin::applyChanges() 
+{
+  kdDebug() << "applychanges\n";
+  if (!d->page->save()) {
+    properties->abortApplying();
+  }
+}
+
