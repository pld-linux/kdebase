+QString KSMClient::program() const
+{
+    SmProp* p = property( SmProgram );
+    if ( !p || qstrcmp( p->type, SmARRAY8) || p->num_vals < 1)
+        return QString::null;
+    return QString::fromLatin1( (const char*) p->vals[0].value );
+}
+
+QStringList KSMClient::restartCommand() const
+{
