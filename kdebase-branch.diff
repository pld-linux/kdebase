+{
+    if (!m_useViewports) return _p;
+
+    QPoint vx(m_pager->kwin()->currentViewport(m_pager->kwin()->currentDesktop()));
+
+    // ### fix vertically layouted viewports
+    QPoint p(_p);
+    p.setX(p.x() + (m_desktop - vx.x()) * QApplication::desktop()->width());
+    return p;
+}
