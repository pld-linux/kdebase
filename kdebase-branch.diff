+    SmProp* p = property( SmDiscardCommand );
+    if ( !p || qstrcmp( p->type, SmLISTofARRAY8) || p->num_vals < 1)
+        return result;
+    for ( int i = 0; i < p->num_vals; i++ )
+        result +=QString::fromLatin1( (const char*) p->vals[i].value );
+    return result;
+}
